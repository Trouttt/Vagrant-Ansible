Vagrant.configure("2") do |config|
    config.vm.box = "centos/7" # SO
    config.vm.hostname = "app01" #nome da Maquina Virtual
    config.vm.network "private_network", ip:"192.168.1.3" #nome da rede intera q vai dispon√≠vel no ambiente do virtualbox
    config.vm.network "forwarded_port", guest: 8080, host: 8080
    config.vm.provision "shell", path: "provision.sh"
  end
  